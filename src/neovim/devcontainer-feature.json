{
    "name": "Neovim (from source)",
    "id": "neovim",
    "version": "0.0.2",
    "dependsOn": {
      "ghcr.io/mmeijerdfki/devcontainer-features/common-base": {}
    },
    "description": "A feature to install Neovim building from source",
    "options": {
        "version": {
            "type": "string",
            "proposals": [
                "stable",
                "nightly",
                "0.9.5",
                "0.9.4",
                "0.9.3",
                "0.9.2",
                "0.9.1",
                "0.9.0"
            ],
            "default": "stable",
            "description": "The version of Neovim to be installed (stable, nightly or a specific version in the format 'MAJOR.MINOR.PATCH' e.g. '0.9.5)"
        }
    },
    "installsAfter": [
        "ghcr.io/mmeijerdfki/devcontainer-features/common-base"
    ]
}
